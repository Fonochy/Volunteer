    </b>
  </div>
  <img src="img/imgv/VolunteerBannerFull.jpg" class="img-fluid text-align-center" alt="Responsive image">

  <% Object.values(program).forEach(function(item){%>
  <div class="container py-3">
    <div class="card">
      <div class="row">
        <div class="col-md-6" style="background-color: #FAEEEE;">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="img/imgv/<%= item.img %>" class="img-fluid imgsize" alt="Responsive image"></a>
            </div>
          </div>
        </div>

        <div class="col-md-6 px-4" style="background-color: #FAEEEE;">
          <div class="card-block px-6">
            <b>
              <h4 class="card-title topic cardbody1 text-danger"><%= item.name %></h4>
            </b>
            <br>
            <div class="container">
              <table class="table table-borderless">
                <div class="row table-responsive">
                  <div class="col-2 num-container ">
                    <tbody>
                      <tr class="row1">
                        <td><img src="img/imgv/gps1.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="n1 cardbody"><%= item.place%></p>
                        </td>

                      </tr>
                      <tr class="row2 ">
                        <td><img src="img/imgv/time.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="n1 cardbody"><%= item.time %></p>
                        </td>
                      </tr>
                      <tr class="row3">
                        <td><img src="img/imgv/home.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="n1 cardbody"><%= item.house %></p>
                        </td>
                      </tr>
                      <tr class="row4">
                        <td><img src="img/imgv/money.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="n1 cardbody"><%= item.price%></p>
                        </td>
                      </tr>
                      <tr class="row5">
                        <td><img src="img/imgv/human.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="n1 cardbody"><%= item.age %></p>
                        </td>
                      </tr>
                      <tr class="row6">

                        <td><img src="img/imgv/info.png" class="icon1" alt="Responsive image"></td>
                        <td>
                          <p class="cardbody"><%= item.pinfo %></p>
                        </td>
                      </tr>
                    </tbody>
                  </div>
                </div>
              </table>
            </div>
            <a href="/mainprogram/<%= item._id %>" class="mt-auto b1 btn btn-primary cardbody">Read More</a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% }) %>

</body>

